# sdcadm - administer an SDC standup
TOP=$(cd $(dirname $0)/../; pwd)
if [[ -x "$TOP/build/node/bin/node" ]]; then
    # Dev layout.
    $TOP/build/node/bin/node $TOP/lib/cli.js "$@"
else
    /opt/smartdc/sdcadm/node/bin/node /opt/smartdc/sdcadm/lib/cli.js "$@"
fi
